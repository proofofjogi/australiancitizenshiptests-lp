    <script defer>
    // understand user behavior to make app better
    //  function logActivity(message) {
    //    $.ajax({
    //      url: "/api/activity",
    //      method: 'GET',
    //      data: { message: message },
    //      success: function (data) {
    //        // nothing needed here
    //      },
    //      error: function (error) {
    //        console.error('Log request failed:', error.statusText || 'Unknown error');
    //      }
    //    });
    //  }
    function logActivity(message) {
      const xhr = new XMLHttpRequest();
      xhr.open('GET', "/api/activity?message=" + encodeURIComponent(message), true);
      xhr.onload = function () {
        if (xhr.status >= 200 && xhr.status < 300) {
          // nothing needed here
        } else {
          console.error('Log request failed:', xhr.statusText || 'Unknown error');
        }
      };
      xhr.onerror = function () {
        console.error('Log request failed:', xhr.statusText || 'Unknown error');
      };
      xhr.send();
    }
    </script>

    
    
